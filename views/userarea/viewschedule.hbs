<div class="view-schedule-container">
  <div class="vs-innerDiv">
    <h1>This is your schedule for the day</h1>
    <div class="vs-viewschedule">
      <h1>{{dayview}}</h1>
      <div class="vs-divoflists">
        <div class="vs-lists">
          <h3>Breakfast</h3>
          <p>{{breakfastname}}</p>
          <ul>
            {{#each ingredientsbreakfast}}
            <li>
              {{this}}
            </li>
            {{/each}}
          </ul>
        </div>
        <div class="vs-lists">
          <h3>Morningsnack</h3>
          <p>{{morningsnackname}}</p>
          <ul>
            {{#each ingredientsmorningsnack}}
            <li>

              {{this}}

            </li>
            {{/each}}
          </ul>
        </div>
        <div class="vs-lists">
          <h3>Lunch</h3>
          <p>{{lunchname}}</p>
          <ul>
            {{#each ingredientslunch}}
            <li>

              {{this}}

            </li>
            {{/each}}
          </ul>
        </div>
        <div class="vs-lists">
          <h3>Afternoonsnack</h3>
          <p>{{afternoonsnackname}}</p>
          <ul>
            {{#each ingredientsafternoonsnack}}
            <li>

              {{this}}

            </li>
            {{/each}}
          </ul>
        </div>
        <div class="vs-lists">
          <h3>Dinner</h3>
          <p>{{dinnername}}</p>
          <ul>
            {{#each ingredientsdinner}}
            <li>

              {{this}}

            </li>
            {{/each}}
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>